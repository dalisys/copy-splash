{
  "manifest_version": 3,
  "name": "Unsplash Image URL Copier",
  "version": "1.1.6",
  "description": "Adds a button to copy Unsplash image URLs.",
  "permissions": ["clipboardWrite", "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["*://unsplash.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
